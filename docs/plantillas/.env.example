# Variables de entorno para pruebas E2E y performance

# URL pública o de entorno autorizado para pruebas
BASE_URL=https://tu-entorno-reginsa
REGINSA_URL=https://tu-entorno-reginsa/#/home

# Usuario de pruebas no productivo
# Aquí se colocan usuario y contraseña principal para ejecutar los casos
E2E_USERNAME=usuario_pruebas
E2E_PASSWORD=password_pruebas
REGINSA_USER=usuario_pruebas
REGINSA_PASS=password_pruebas

# Opcional: pool de usuarios para ejecución paralela
# Si vas a correr 100-1000 administrados en pipeline, agrega más pares _N
REGINSA_USER_1=usuario_1
REGINSA_PASS_1=password_1
REGINSA_USER_2=usuario_2
REGINSA_PASS_2=password_2
REGINSA_USER_3=usuario_3
REGINSA_PASS_3=password_3
REGINSA_USER_4=usuario_4
REGINSA_PASS_4=password_4

# Variables para K6
K6_BASE_URL=https://tu-entorno-reginsa

# SonarQube (usar variables secretas en pipeline, no en repositorio)
SONAR_HOST_URL=https://tu-servidor-sonarqube
SONAR_PROJECT_KEY=reginsa_qa
